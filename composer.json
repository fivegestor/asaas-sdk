{
    "name": "fivegestor/asaas-sdk",
    "description": "SDK para integração com a API Asaas - FiveDigital",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FiveGestor\\AsaasSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FiveGestor\\AsaasSdk\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Daniel Melo",
            "email": "daniel.melo@nestdigital.com.br"
        }
    ],
    "require": {
        "illuminate/config": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/support": "^6|^7|^8|^9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^4|^5|^6|^7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FiveGestor\\AsaasSdk\\ServiceProviders\\ServiceProvider"
            ]
        }
    }
}
